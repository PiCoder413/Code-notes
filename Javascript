/*
+ = addition
- = subtraction 
* = multiplication
/ = division
% = remainder after divsion
:*/
console.log(2+3)
console.log(3-2)
console.log(3*2)
console.log(10/2)
console.log(10%3)
//String Concantacetion 
console.log('Cookie'+'God')
console.log('This is'+ x)
